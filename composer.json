{
    "name": "iamkesharinandan/laravel-dynamodb",
    "description": "A repository for Laravel to interact with AWS DynamoDB",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Keshari Nandan",
            "email": "iamkesharinandan@gmail.com",
            "homepage": "https://github.com/iamkesharinandan"
        }
    ],
    "config": {
        "secure-http": false,
        "github-protocols": [
            "git",
            "https"
        ],
        "discard-changes": true,
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.212"
    },
    "require-dev": {
        "laravel/framework": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "IamKeshariNandan\\LaravelDynamodb\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IamKeshariNandan\\LaravelDynamodb\\DynamodbServiceProvider"
            ]
        }
    }
}
